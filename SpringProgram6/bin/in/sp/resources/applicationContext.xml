<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
    <bean class="in.sp.beans.Address" id="addr">
		<constructor-arg value="222"/>
		<constructor-arg value="Delhi"/>
		<constructor-arg value="123321"/>
	</bean>

	<bean class="in.sp.beans.Student" id="stdId">
		<constructor-arg value="1001"/>
		<constructor-arg value="Dipesh"/>
		<constructor-arg ref="addr"/>
	</bean>
    

</beans>