    </bean>
    
     <bean class="in.sp.beans.Student" id="stdId2">
    	<property name="name" value="Vivek"></property>
    	<property name="rollno" value="102"></property>
    	<property name="email" value="vivek@mail.com "></property>
    </bean>

</beans> 